<p id="notice"><%= notice %></p>

<h1>Listing Support Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @support_numbers.each do |support_number| %>
      <tr>
        <td><%= support_number.name %></td>
        <td><%= support_number.number %></td>
        <td><%= link_to 'Show', support_number %></td>
        <td><%= link_to 'Edit', edit_support_number_path(support_number) %></td>
        <td><%= link_to 'Destroy', support_number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Support number', new_support_number_path %>
